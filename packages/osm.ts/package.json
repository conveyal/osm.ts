{
	"$schema": "https://json.schemastore.org/package",
	"name": "osm.ts",
	"type": "module",
	"version": "0.0.1",
	"description": "An OSM PBF reader, writer, and merge tool written in TypeScript",
	"main": "./src/index.ts",
	"exports": {
		".": "./src/index.ts",
		"./changes": "./src/osm-change.ts",
		"./geojson": "./src/to-geojson.ts",
		"./pbf": "./src/pbf/index.ts",
		"./utils": "./src/utils.ts",
		"./performance": "./src/performance.ts"
	},
	"sideEffects": false,
	"scripts": {
		"test": "vitest",
		"typecheck": "tsc --noEmit"
	},
	"devDependencies": {
		"@osmix/test-utils": "workspace:*",
		"@turf/invariant": "7.2.0",
		"@turf/meta": "7.2.0",
		"@types/geojson": "catalog:",
		"typescript": "catalog:",
		"vitest": "catalog:"
	},
	"dependencies": {
		"@mapbox/sphericalmercator": "^2.0.1",
		"@osmix/json": "workspace:*",
		"@osmix/pbf": "workspace:*",
		"@turf/distance": "7.2.0",
		"@turf/line-intersect": "7.2.0",
		"dequal": "catalog:",
		"flatbush": "^4.4.1",
		"kdbush": "mourner/kdbush#16416cd",
		"lineclip": "^2.0.0"
	}
}
