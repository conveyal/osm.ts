{
	"$schema": "https://json.schemastore.org/package",
	"name": "osm.ts",
	"type": "module",
	"version": "0.0.1",
	"description": "An OSM PBF reader, writer, and merge tool written in TypeScript",
	"main": "./src/index.ts",
	"exports": {
		".": "./src/index.ts",
		"./changes": "./src/osm-change.ts",
		"./geojson": "./src/to-geojson.ts",
		"./pbf": "./src/pbf/index.ts",
		"./utils": "./src/utils.ts"
	},
	"sideEffects": false,
	"scripts": {
		"bench": "vitest bench",
		"test": "vitest",
		"typecheck": "tsc --noEmit"
	},
	"devDependencies": {
		"@types/geojson": "catalog:",
		"typescript": "catalog:",
		"vitest": "^3.1.3"
	},
	"dependencies": {
		"@mapbox/sphericalmercator": "^2.0.1",
		"@mapbox/tilebelt": "^2.0.2",
		"@math.gl/web-mercator": "^4.1.0",
		"@turf/turf": "catalog:",
		"dequal": "^2.0.3",
		"flatbush": "^4.4.1",
		"kdbush": "^4.0.2",
		"pbf": "^4.0.1"
	}
}
